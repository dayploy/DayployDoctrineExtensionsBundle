{
    "name": "dayploy/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "homepage": "https://github.com/dayploy/DayployDoctrineExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dayploy",
            "email": "contact@dayploy.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/config": "^7.0",
        "doctrine/doctrine-bundle": "^2.12",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "doctrine/orm": "^3.0"
    },
    "require-dev": {
        "nyholm/symfony-bundle-test": "^3.0",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Dayploy\\DoctrineExtensionsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dayploy\\DoctrineExtensionsBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}